      <parent link="leg_bracket_${lr}"/>
      <child link="servo_upper_back_${lr}"/>
      <axis xyz="0 1 0"/>
    </joint>

    <link name="leg_bracket_${lr}">
      <collision>
        <origin xyz="${tY*-32e-3} 21e-3 -31e-3" rpy="0 0 0"/>
